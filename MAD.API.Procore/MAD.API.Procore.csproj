<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>Maitland Marshall</Authors>
		<Company>MAIT DEV</Company>
		<PackageProjectUrl>https://github.com/maitlandmarshall/MAD.API.Procore</PackageProjectUrl>

		<Major>2</Major>
		<Minor>0</Minor>
		<Build>5</Build>
		<Revision>$([System.DateTime]::UtcNow.ToString("MddHHmm"))</Revision>

		<Version>$(Major).$(Minor).$(Build).$(Revision)</Version>
		<AssemblyVersion>$(Major).$(Minor).$(Build)</AssemblyVersion>
		<Platforms>AnyCPU;x64;x86</Platforms>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DinkToPdf" Version="1.0.8" />
    <PackageReference Include="HtmlAgilityPack" Version="1.11.67" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="PDFTron.NET.x64" Version="10.12.1" />
    <PackageReference Include="PDFTron.NETCore.Windows.x64" Version="10.12.1" />
  </ItemGroup>

</Project>
